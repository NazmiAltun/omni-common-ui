.PerformanceProfiler {
  background: #bada55;
  border: 2px solid black;
  display: flex;
  flex-direction: column;
  padding: 10px;
  position: absolute;
  right: 50px;
  text-align: center;
  top: 20px;

  & > h1 {
    font-size: 1.5em;
  }

  & > button {
    display: block;
    margin-top: 10px;
    padding: 5px;
  }
}
