.SelectionTable {
  &_heading {
    display: flex;

    &_route {
      cursor: pointer;
      margin-right: 20px;
    }

    &_back {
      display: inline-flex;

      &:global {
        background-image: url('./arrow.svg');
        background-repeat: no-repeat;
        background-size: 1rem;
        height: 20px;
        margin-top: 2px;
        width: 20px;
      }
    }
  }
}
