@import 'main.postcss';

.SessionNav {
  align-items: center;
  display: flex;

  &_emptyBtn {
    width: 2em;
  }

  &_back, &_forward {
    border-bottom: 0.25em solid;
    border-right: 0.25em solid;
    cursor: pointer;
    display: flex;
    height: 2em;
    width: 2em;
  }

  &_back {
    transform: rotate(135deg);
  }

  &_forward {
    transform: rotate(-45deg);
  }

  &_content {
    list-style-type: none;
  }
}
