@import 'main.postcss';

.Overlay {
  align-items: center;
  background-color: rgba(255, 255, 255, 0.75);
  bottom: 0;
  display: flex;
  justify-content: center;
  left: 0;
  padding: 2.5em;
  position: fixed;
  right: 0;
  top: 0;
}

.Dialog {
  display: flex;
  max-height: 100%;
  max-width: 100%;
  outline: none;
}
