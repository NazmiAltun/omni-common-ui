{
  "extends": ["airbnb"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": [1, "always-multiline"],
    "import/no-unresolved": [0]
  },
  "globals": {
    "PRODUCTION": false,
    "DEVELOPMENT": false,
    "TEST": false
  }
}
